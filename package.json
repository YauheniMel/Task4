    "classnames": "^2.3.1",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.3",
    "mysql2": "^2.3.3",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "react-toastify": "^9.0.3",
    "redux": "^4.2.0",
    "redux-thunk": "^2.4.1",
    "socket.io": "^4.5.1",
