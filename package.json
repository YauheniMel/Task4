    "ajv": "^8.17.1",
    "eslint": "^8.16.0",
    "eslint-plugin-react": "^7.30.0",
    "husky": "^8.0.1",
    "nodemon": "^3.1.9",
    "prettier": "2.6.2",
    "sass": "^1.52.1",
    "stylelint": "^14.8.5",
    "typescript": "^4.7.2",
    "webpack-cli": "^6.0.1"
  },
  "lint-staged": {
    "**/*.{tsx,ts,js,jsx}": "eslint --fix",
    "**/*.{tsx,ts,md,js,jsx}": "npx prettier --write"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "server:dev": "nodemon ./server",
    "server": "node ./dist",
    "server:build": "webpack",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint **/*.{js,jsx,tsx,ts}",
    "style": "stylelint **/*.{scss,css} -f verbose --fix",
    "prettier": "prettier --check \"{,!(node_modules)/**/}*.{tsx,ts,css,scss,md,js,jsx}\"",
    "format": "prettier --write \"{,!(node_modules)/**/}*.{tsx,ts,md,js,jsx}\" && eslint src/**/*.{tsx,ts,md,js,jsx} --fix",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
